{#
/**
 * @file
 * Default theme implementation for a GHO related articles formatter.
 *
 * Available variables:
 * - title: Link title
 * - list: list of related articles.
 *
 * @ingroup themeable
 */
#}
<div class="gho-related-articles">
{% if title %}
<h2 class="gho-related-articles__title">{{ title }}</h2>
{% endif %}
<div  class="gho-related-articles__list">
{{ list }}
</div>
</div>
